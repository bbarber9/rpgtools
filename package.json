{
  "name": "rpgtools",
  "version": "0.1.0",
  "description": "Tool set for playing an RPG game",
  "licence": "MIT",
  "repository": "https://github.com/zachanator070/rpgtools",
  "scripts": {
    "start": "nodejs -r esm src/server/index.js",
    "start-dev": "npx nodemon --delay 500ms --inspect=0.0.0.0 -- -r esm src/server/index.js",
    "build": "npx webpack",
    "watch": "npx webpack --config webpack-dev.config.js -w"
  },
  "dependencies": {
    "apollo-server": "^2.9.5",
    "apollo-server-express": "latest",
    "body-parser": "^1.19.0",
    "esm": "^3.2.25",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "express": "latest"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.19.3",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  }
}
